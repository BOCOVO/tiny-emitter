{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["ESNext"],
    "module": "ES6",
    "noEmit": true,
    "noEmitOnError": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "paths": {
      "@/*":["src/*"]
    }
  },
  "include": ["src"]
}